{
  "name": "pickup-service",
  "version": "1.0.0",
  "private": true,
  "description": "픽업 서비스 - PostgreSQL 데이터베이스 사용",
  "author": "DOCORE",
  "license": "MIT",
  "scripts": {
    "start:db": "docker-compose up -d postgres",
    "stop:db": "docker-compose down",
    "migrate:json-to-postgres": "node scripts/migrate-json-to-postgres.js",
    "start:api": "node src/backend/api_server.js",
    "dev:api:db": "npm run start:db && sleep 5 && npm run migrate:json-to-postgres && npm run start:api",
    "test:api": "curl http://localhost:8081/api/healthz"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "mime-types": "^2.1.35",
    "openai": "^6.7.0",
    "pg": "^8.11.3",
    "qrcode": "^1.5.4",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.19.23",
    "@types/pg": "^8.10.7",
    "@types/uuid": "^9.0.7",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "packageManager": "npm@10.2.0"
}
